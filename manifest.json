{
    "manifest_version": 3,
    "name": "Dashboard Utama",
    "version": "1.1",
    "description": "A personalized dashboard for new tab.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "host_permissions": [
        "https://api.openweathermap.org/*"
    ],
    "permissions": [
        "geolocation"
    ]
}
